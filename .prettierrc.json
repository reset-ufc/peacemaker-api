{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "quoteProps": "as-needed",
  "objectWrap": "preserve",
  "singleAttributePerLine": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "arrowParens": "avoid",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "semi": true,
  "useTabs": false,
  "endOfLine": "lf",
  "printWidth": 80
}
